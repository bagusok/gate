{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": ["DATABASE_URL"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false
		},
		"db:generate": {},
		"db:migrate": {},
		"db:push": {},
		"db:studio": {
			"cache": false,
			"persistent": true
		}
	}
}
